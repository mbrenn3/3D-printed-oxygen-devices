journal = {Analytical Chemistry},
number = {13},
pages = {6683--6688},
pmid = {24856003},
title = {{Ultrarapid detection of pathogenic bacteria using a 3D immunomagnetic flow assay}},
volume = {86},
year = {2014}
}
